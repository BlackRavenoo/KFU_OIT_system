{
	"name": "kfu-oit-system",
	"description": "KFU OIT System - A web application for managing IT support requests",
	"author": "ivanvit, BlackRavenoo",
	"private": true,
	"version": "0.1.12",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run test && vite build",
		"build:no-test": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"watch:build": "chokidar 'src/**/*' -c 'vite build'",
		"test": "vitest run --isolate",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:ui": "vitest --ui"
	},
	"devDependencies": {
		"@fingerprintjs/fingerprintjs": "^4.6.2",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@testing-library/svelte": "^5.2.8",
		"@types/node": "^24.5.2",
		"@vitest/coverage-istanbul": "^3.2.4",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"axios": "^1.11.0",
		"axios-mock-adapter": "^2.1.0",
		"chokidar-cli": "^3.0.0",
		"cropperjs": "^2.0.1",
		"cssnano": "^7.1.0",
		"js-yaml": "^4.1.0",
		"jsdom": "^26.1.0",
		"msw": "^2.11.1",
		"postcss": "^8.5.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"terser": "^5.43.1",
		"typescript": "^5.0.0",
		"vite": "^6.2.6",
		"vite-plugin-html-minifier-terser": "^3.8.0",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"file-saver": "^2.0.5",
		"require": "^2.4.20",
		"xlsx-js-style": "^1.2.0"
	}
}
